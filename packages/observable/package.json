{
  "name": "@emitterware/observable",
  "description": "Observable objects in Node & the browser",
  "authors": "Andrew Ewing <contact@aewing.io>",
  "version": "0.0.1",
  "main": "dist/main.js",
  "files": [
    "dist/",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "babel --ignore '**/*.test.js,**/__tests__' --compact --minified -d dist/ src/",
    "watch": "babel --ignore '**/*.test.js,**/__tests__' --watch --compact --minified -d dist/ src/",
    "lint": "eslint src/",
    "lint-fix": "eslint src/ --fix",
    "clean": "rm -rf dist/*.js",
    "build-clean": "npm run lint-fix && npm run clean && npm run build"
  },
  "dependencies": {
    "@emitterware/emitter": "^0.0.1"
  },
  "devDependencies": {
    "@emitterware/middleware": "0.0.5"
  }
}
